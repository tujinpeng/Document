##memcached重启
memcached -d -p 12000 -u root -m 1024 -c 1024

##brew 启动服务
brew services start activemq

##查看占用的端口号
lsof -i tcp:1099

#查看ip
ifconfig


#本地建工程推送到git上
git remote remove origin
git remote add origin git@github.com:tujinpeng/lts.git
git push -u origin master

#新建本地分支
git branch [branch_name]

#推送本地分支到远程指定分支上
git push origin [remote_branch]
git push origin [local_branch]:[remote_branch]

#删除本地分支 远程分支
git branch -D [branch_name]
git push origin :[remote_branch_name]


#本地分支和远程分支进行关联
git branch --set-upstream [local_name] origin/[remote_branch_name]

#查找文本
grep -a10000 'queryPriceInfo start::tntSellPackageId:\[455460\]' */catalina.out-2017-10-19|grep 'queryPriceInfo'


#tomcat
-Xms2048M -Xmx4096M -XX:PermSize=2048M -XX:MaxPermSize=4096M

